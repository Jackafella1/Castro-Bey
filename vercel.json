{
       "version": 2,
       "builds": [
         {
           "src": "dist/**",
           "use": "@vercel/static"
         }
       ],
       "outputDirectory": "dist",
       "routes": [
         {
           "src": "/",
           "dest": "/dist/index.html"
         },
         {
           "src": "/assets/(.*)",
           "dest": "/dist/assets/$1"
         },
         {
           "src": "/images/(.*)",
           "dest": "/images/$1"
         },
         {
           "src": "/(.*)",
           "dest": "/dist/$1"
         }
       ]
     }